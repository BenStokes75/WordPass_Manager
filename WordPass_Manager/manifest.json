{
    "manifest_version": 3,
    "name": "WordPass Manager",
    "version": "1.0",
    "description": "A simple password manager extension.",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  